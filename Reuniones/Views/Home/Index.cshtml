@model IEnumerable<Reuniones.Models.Reunion>

@{
    ViewBag.Title = "Index";
}

<h2>Agenda de Reuniones</h2>

<p>
    @Html.ActionLink("Agendar Nueva Reunion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Inicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Cierre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Inicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Cierre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) 
        </td>
    </tr>
}

</table>
